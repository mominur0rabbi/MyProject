    "import vcf\n",
    "\n",
    "def read(f):\n",
    "    reader = vcf.Reader(open(f))\n",
    "    df = pd.DataFrame([vars(r) for r in reader])\n",
    "    out = df.merge(pd.DataFrame(df.INFO.tolist()),\n",
    "                   left_index=True, right_index=True)\n",
    "    return out"
